<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.FiveLastName.ShippingStatementmapper">

	<!-- 출하명세서 전체 조회 -->
	<select id="shList" resultType="kr.co.FiveLastName.domain.ShippingStatementDTO">
		select * from shippingstatement
	</select>
	
	<!-- 출하아이디로 선택 조회 -->
	<select id="shSelectOne" parameterType="int" resultType="kr.co.FiveLastName.domain.ShippingStatementDTO">
		select * from shippingstatement
		where sh_id = #{sh_id} 
	</select>
	
	<!-- 출하명세서 추가 -->
	<update id="shInsert" parameterType="kr.co.FiveLastName.domain.ShippingStatementDTO">
		insert into shippingstatement
		values()  
	</update>
	
	<!-- 출하명세서 삭제 -->
	
</mapper>