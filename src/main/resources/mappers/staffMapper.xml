<?xml version="1.0" encoding="UTF-8"?>

<mapper namespace="kr.co.FiveLastName.staffMapper">

	<!-- 담당자 전체 조회 -->
	<select id="st_selectAll">
		select * from staff
	</select>

	<!-- 담당자 상세 조회 -->
	<select id="st_selectOne" resultType="kr.co.FivelastName.staffDTO"
		parameterType="int">
		select * from staff
		where st_id = #{st_id} and st_pw = #{st_pw}
	</select>

</mapper>