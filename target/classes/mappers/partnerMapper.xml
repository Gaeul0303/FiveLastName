<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="kr.co.FiveLastName.partnerMapper">


	<!-- 협력회사 전체 조회 -->

	<select id="pa_selectAll" resultType="kr.co.FiveLastName.domain.PartnerDTO">
		select * from partner order by pa_id desc;
	</select>
	<select id="pa_selectone" parameterType="int" resultType="kr.co.FiveLastName.domain.PartnerDTO">
		select * from partner where pr_id =#{pr_id};
	</select>

	
</mapper>